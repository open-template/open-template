/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "allowJs": true,
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": ["node"],
    "module": "es2015",
    "paths": {
      "@Model/*": ["src/lib/xmlns/*"]
    },
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "exclude": [
    "**/*.spec.ts"
  ]
}
